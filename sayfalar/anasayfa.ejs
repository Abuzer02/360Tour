                <form id="contact-form">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="name">Ad </label>
                            <input type="text" class="form-control" id="name"  required="required" />
                        </div>
                        <div class="form-group">
                            <label for="email">E-mail </label>
                            <div class="input-group">
                                <span class="input-group-addon"><span class="glyphicon glyphicon-envelope"></span>
                                </span>
                                <input type="email" class="form-control" id="email" required="required" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="subject">Konu</label>
                            <select id="subject" name="subject" class="form-control" required="required">
                                <option value="select_subject" selected="">Konu Seçiniz:</option>
                                <option value="suggestions">Öneri</option>
                                <option value="complaints">Şikayet</option>
                                <option value="support">Destek</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="name">
                                Mesaj</label>
                            <textarea name="message" id="message" class="form-control" rows="9" cols="25" required="required" placeholder="Mesajınızı giriniz." style="resize:vertical"></textarea>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <a type="submit" class="btn btn-skin pull-right">Gönder</a>
                    </div>
                </div>
                </form>
